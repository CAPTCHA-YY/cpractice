#include<stdio.h>//简单的找素数问题的模型，加一个1
#include<stdlib.h>
#include<string.h>
int main(){
    int n;
    scanf("%d",&n);
    int arr[n];
   
    for(int i=0;i<n;i++)
    scanf("%d",&arr[i]);
    int g=0;
    for(int i=0;i<n;i++){//找出n组数据中最大值，以他为结尾记录前面所有范围内多少质数就够了
    	if(g<arr[i])
    	g=arr[i];
	} 
	int dp[g+1];//算动态规划？递归也算动态规划一种
	dp[1]=1;
	dp[2]=2; 
	unsigned char brr[g+1];
	memset(brr,1,g+1);//又学了新东西，虽然局限挺大
	for(int i=2;i*i<=g;++i){
		if(brr[i]){
			for(int p=i*i;p<=g;p+=i){//核心点，用名字奇奇怪怪的算法
				brr[p]=0;
			}
		}
	}
    for(int i=3;i<=g;i++){
    	dp[i]=dp[i-1]+brr[i];//应该叫递归
	}
    
    
    for(int i=0;i<n;i++){
    	printf("%d\n",dp[arr[i]]);//最后输出1到arr[i]的素数加1
	} 
    
    return 0;
}
